<div class="container foot">
  <ul class="list-inline tabs-underlined ">
    <li>
      <%= link_to "My bookings", dashboard_path, class: "tab-underlined" %>
    </li>
    <li>
      <%= link_to "My kitchens", mykitchens_path, class: "tab-underlined active" %>
    </li>
    <li>
      <%= link_to "Add kitchen", new_kitchen_path, class: "tab-underlined"%> 
    </li>
  </ul>
  <% current_user.kitchens.each do |kitchen| %>
      <div class="card-mykitchen">
        <div class="card-mykitchen-infos">
          <img src="http://static.cotemaison.fr/medias_11928/w_1359,h_760,c_crop,x_0,y_145/w_2000,h_1125,c_fill,g_north/v1561563110/cuisine-leroy-merlin-12_6107562.jpg" />
          <div class="card-details">
            <h5 class="card-mykitchen-subtitle"><%= current_user.email %></h5> 
          </div>
        </div>
        <div class="calltoactions">
        <%= link_to "Edit", new_kitchen_path, class: "btn btn-success" %>

       <%= link_to "Delete", mykitchens_path, class: "btn btn-danger" %>
        </div>
      </div>
  <% end %>
</div>